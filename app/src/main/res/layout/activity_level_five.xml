<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/levelFivePage"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LevelFive"
    android:background="@drawable/levelone_bg">

    ///Discolored Cap
    <ImageView
        android:id="@+id/darkyellowCapLevelFive"
        android:layout_width="74dp"
        android:layout_height="74dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="70dp"
        android:src="@drawable/darkyellowcap" />

    <ImageView
        android:id="@+id/blackCapLevelFive"
        android:layout_width="124dp"
        android:layout_height="124dp"
        android:layout_marginLeft="110dp"
        android:layout_marginTop="40dp"
        android:src="@drawable/blackcap" />

    <ImageView
        android:id="@+id/blueCapLevelFive"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginLeft="270dp"
        android:layout_marginTop="50dp"
        android:src="@drawable/bluecap" />

    ///MainCap
    <ImageView
        android:id="@+id/mainCapLevelfive"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="300dp"
        android:src="@drawable/cap" />
    ///Congratulation image
    <ImageView
        android:id="@+id/levelfivewinimage"
        android:layout_width="144dp"
        android:layout_height="144dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="310dp"
        />
    ///Congratulation text
    <TextView
        android:id="@+id/levelfivewintext"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginLeft="140dp"
        android:layout_marginTop="250dp"
        android:fontFamily="@font/bubblegumsans_regular"
        android:textColor="#f0a144"
        android:textStyle="bold"
        android:textSize="45sp" />
    ///Back
    <Button
        android:id="@+id/backbuttontoMenu"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_gravity="right"
        android:layout_marginTop="495dp"
        android:layout_marginLeft="10dp"
        android:background="@drawable/backbutton"
        android:textColor="#632323"
        android:textStyle="bold"
        android:text="Back"
        android:textSize="20sp"
        />

</RelativeLayout>