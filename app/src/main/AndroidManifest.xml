<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sc4ever.bornali">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LevelTwo"
            android:label="@string/title_activity_level_two"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".LevelOne" />
        <activity android:name=".ProfilePage">
            <intent-filter>
                <action android:name="android.intent.action.ProfilePage" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PuzzleMenuPage"
            android:label="@string/title_activity_puzzle_menu_page"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>